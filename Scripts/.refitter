{
  "openApiPath": "api-server/swagger.json",
  "namespace": "Zarichney.ApiClient.Interfaces",
  "contractsNamespace": "Zarichney.ApiClient.Models",
  "outputFolder": "../Zarichney.ApiClient",
  "generateContracts": true,
  "generateClients": true,
  "generateMultipleFiles": true,
  "multipleInterfaces": "ByTag",
  "addAutoGeneratedHeader": true,
  "addAcceptHeaders": true,
  "addContentTypeHeaders": false,
  "returnIApiResponse": true,
  "generateOperationHeaders": true,
  "generateXmlDocCodeComments": true,
  "generateStatusCodeComments": true,
  "typeAccessibility": "Public",
  "useCancellationTokens": true,
  "trimUnusedSchema": true,
  "generateDeprecatedOperations": false,
  "optionalParameters": true,
  "operationNameGenerator": "Default",
  "operationNameTemplate": "{operationName}",
  "useDynamicQuerystringParameters": false,
  "immutableRecords": true,
  "useIsoDateFormat": true,
  "generateDefaultAdditionalProperties": true,
  "skipDefaultAdditionalProperties": false,
  "dependencyInjectionSettings": {
    "baseUrl": "http://localhost:5000",
    "transientErrorHandler": "None",
    "httpMessageHandlers": []
  },
  "codeGeneratorSettings": {
    "generateNullableReferenceTypes": true,
    "generateDataAnnotations": true,
    "generateDefaultValues": true,
    "requiredPropertiesMustBeDefined": true,
    "arrayType": "System.Collections.Generic.ICollection",
    "dictionaryType": "System.Collections.Generic.IDictionary",
    "generateOptionalPropertiesAsNullable": true,
    "dateFormat": "yyyy-MM-dd",
    "dateTimeFormat": "yyyy-MM-ddTHH:mm:ss.fffZ"
  }
}